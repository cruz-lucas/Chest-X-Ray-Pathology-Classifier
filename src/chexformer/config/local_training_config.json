{
  "base_model": "google/vit-base-patch16-224",
  "uncertainty_policy": "U-Ignore",
  "num_epochs": 1,
  "max_steps": 100,
  "learning_rate": 0.0003,
  "weight_decay": 0.02,
  "warmup_ratio": 0.03,
  "lr_scheduler_type": "cosine",
  "batch_size": 16,
  "gradient_accumulation": 1,
  "save_steps": 500,
  "lora_r": 8,
  "lora_alpha": 16,
  "lora_dropout": 0.05,
  "lora_bias": "none",
  "lora_use_rslora": true,
  "device": "cpu",
  "lora_target_modules": [
    "query",
    "value"
  ],
  "resume_from_checkpoint": null
}
